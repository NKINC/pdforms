            If Not Session("output") Is Nothing Then
                Select Case cFDFDoc.Determine_Type(fpath)
                    Case FDFApp.FDFDoc_Class.FDFType.PDF
                        cFDFDoc = cFDFApp.PDFOpenFromBuf(Session("output"), True, True)
