        strINPUT = strINPUT.Replace("&", "&&38;")
        strINPUT = strINPUT.Replace("#", "&#35;")
        strINPUT = strINPUT.Replace("&&38;", "&#38;")
        strINPUT = strINPUT.Replace("<", "&#60;")
        strINPUT = strINPUT.Replace(">", "&#62;")
        strINPUT = strINPUT.Replace("(", "&#40;")
        strINPUT = strINPUT.Replace(")", "&#41;")
