                        End If
                    Case Else
                        Return
                End Select
            ElseIf Clipboard.ContainsText(System.Windows.Forms.TextDataFormat.Text) Then
                Dim oDataString As String = System.Windows.Forms.Clipboard.GetText(TextDataFormat.Text).ToString() & ""
