                                    For Each fld As FDFApp.FDFDoc_Class.FDFField In fdfDoc.XDPGetAllFields()
                                        If Not lstFieldNames.Contains(fld.FieldName.ToString().ToLower()) Then
                                            lstFieldNames.Add(fld.FieldName.ToString().ToLower())
