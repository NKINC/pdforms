            Loop
            Return frmImageRot.rotType
        Catch ex As Exception
            TimeStampAdd(ex, debugMode)
        Finally
            If closeForm Then
                frmImageRot.Close()
            Else
                frmImageRot.Hide()
            End If
            Me.Show()
            Me.BringToFront()
        End Try
