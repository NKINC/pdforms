                        End If
                    End If
                Next
                If Not blnFound Then
                    If fld.IndRef Is Nothing Then
                        fld.IndRef = pdfReaderDoc.AddPdfObject(fld)
                    End If
                    fields.Add(fld.IndRef())
                End If
                acroForm.Put(PdfName.CO, fields)
