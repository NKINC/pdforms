                    If Not acroForm.Get(PdfName.FIELDS) Is Nothing Then
                        Dim fields As PdfArray = acroForm.GetAsArray(PdfName.FIELDS)
                        Return fields
                    End If
                End If
                Return New PdfArray()
            Catch ex As Exception
                TimeStampAdd(ex, debugMode)
            End Try
