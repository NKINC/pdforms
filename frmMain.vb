            End If
            Dim h As Single = CSng(pdfReaderDoc.GetPageSizeWithRotation(CInt(pageNumber)).Height) + 0
            Dim w As Single = CSng(pdfReaderDoc.GetPageSizeWithRotation(CInt(pageNumber)).Width) + 0
